#include<stdio.h>
#include<stdlib.h>
#include<time.h>
int main(){
	srand(time(NULL));
	int v[9],aux=0;
	printf("Estos son los numeros a ordenar \n \n");
	for(int i=0;i<=9;i++){
	    v[i]=rand()%100;
	    
	    printf("[%i]",v[i]);
	}
	printf("                   \n");
	printf("Estos son tus numeros de mayor a menor\n\n");
	for(int t=0;t<9;t++){
	    for(int j=0;j<9;j++){
		    if(v[j]>v[j+1]){
			   aux=v[j];
			   v[j]=v[j+1];
			   v[j+1]=aux;
		    }
	    }
	}
	for(int k=0;k<10;k++){
			printf("[%i]",v[k]);
	}
return 0;
}
